{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Node Inspector - Downloader",
			"type": "node",
			"request": "launch",
			"args": ["src/index.ts", "--transpile-only"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}/packages/downloader",
			"internalConsoleOptions": "openOnSessionStart",
			"env": {
				"TS_NODE_IGNORE": "false"
			}
		}
	]
}
